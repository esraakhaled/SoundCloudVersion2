<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.soundcloud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Breeze"
            android:label="@string/category_breeze"/>
        <activity android:name=".Midnight"
            android:label="@string/category_midnight"/>
        <activity android:name=".Relax"
            android:label="@string/category_relaxing"/>
        <activity android:name=".Escape"
            android:label="@string/category_escape"/>
    </application>

</manifest>